{
  "name": "vue3-vite-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:reset": "pnpm dlx rimraf node_modules/.vite && vite",
    "reset": "pnpm clean && pnpm i",
    "clean": "pnpm dlx rimraf node_modules",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.9.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@iconify/vue": "^4.0.2",
    "@types/node": "^16.11.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "typescript": "^4.8.4",
    "unocss": "^0.50.6",
    "vite": "^3.2.4",
    "vitest": "^0.26.2",
    "vue-tsc": "^1.0.9"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --max-warnings=0"
    ]
  }
}
